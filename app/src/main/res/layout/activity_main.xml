<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="nanjing.jun.mszdviewlist.MainActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:text="combine"
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:onClick="MszdMenu_1"
            android:text="MszdMenu_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <TextView
            android:text="button"
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <Button
            android:onClick="MszdButton_1"
            android:text="MszdButton_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <Button
            android:onClick="MszdButton_2"
            android:text="MszdButton_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
        <Button
            android:onClick="MszdButton_3"
            android:text="MszdButton_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:text="progress"
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:onClick="MszdProgress_1"
            android:text="MszdProgress_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:onClick="MszdProgress_2"
            android:text="MszdProgress_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:text="loading"
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:onClick="MszdLoading_1"
            android:text="MszdLoading_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:text="viewgroup"
            android:background="#ddd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:onClick="MszdLayout_1"
            android:text="MszdLayout_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </LinearLayout>


</ScrollView>
